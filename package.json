{
  "name": "webpack-svg-sprite",
  "description": "Webpack SVG sprite sample project",
  "author": "vagusX <vagusxl@gmail.com>",
  "scripts": {
    "dev:vue": "node ./node_modules/.bin/webpack-dev-server --port 8001 --config webpack.vue.conf.js --inline --hot",
    "build:vue": "NODE_ENV=production webpack --config webpack.vue.conf.js --progress --hide-modules",
    "dev:react": "node ./node_modules/.bin/webpack-dev-server --port 8002 --config webpack.react.conf.js --inline --hot",
    "build:react": "NODE_ENV=production webpack --config webpack.react.conf.js --progress --hide-modules"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.0.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.4",
    "style-loader": "^0.21.0",
    "svg-sprite-loader": "3.7.3",
    "svgo-loader": "^2.1.0",
    "url-loader": "^1.0.1",
    "vue-hot-reload-api": "^2.3.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^15.0.1",
    "vue-style-loader": "^4.1.0",
    "webpack": "^4.6.0",
    "webpack-dev-server": "^3.1.3"
  }
}
